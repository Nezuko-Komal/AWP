
using System;
using System.Collections; //For Array List

namespace Partical1
{
    class Program
    {
        static void Main(string[] args)
        {
            ArrayList a = new ArrayList();
            a.Add(12); 
            a.Add(43); 
            a.Add("SAHYOG");
            a.Insert(1, 50);

            a.Remove(12);
            a.RemoveAt(2);
            foreach(object r in a)
            {
                Console.WriteLine(r);
            }
        }
    }
}
